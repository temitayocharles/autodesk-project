# Repo-level env contract (docker-compose / local dev).
# Service-specific env contracts live under:
# - services/data-api-service/.env.example
# - services/data-ingestion-service/.env.example
# - services/data-processing-service/.env.example

# Database (used by API + ingestion)
DATABASE_URL=

# Redis (used by API)
REDIS_URL=
CACHE_TTL=300

# AWS (used by ingestion)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-west-2
S3_BUCKET_NAME=aec-data-local

# RabbitMQ (used by processing)
RABBITMQ_URL=
QUEUE_NAME=aec-data-processing
METRICS_PORT=9091
HEALTH_PORT=8081
